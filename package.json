{
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^26.0.1",
    "jest-each": "^28.1.3",
    "prettier": "2.7.1"
  },
  "scripts": {
    "eslint": "eslint lib test",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module"
    },
    "rules": {
      "eqeqeq": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.1"
  }
}
